<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomizeMarqueeView">
        <attr name="isSingleLine" format="boolean" />
        <attr name="marquee_count" format="integer" />
        <attr name="marquee_interval" format="integer|reference" />
        <attr name="marquee_animDuration" format="integer|reference" />
        <attr name="isFlippingLessCount" format="boolean" />
    </declare-styleable>

    <!-- SettingView -->
    <declare-styleable name="SettingView">
        <attr name="iOSStyle" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingViewItem">
        <attr name="title" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="titleColor" format="reference" />
        <attr name="subTitleColor" format="reference" />
        <attr name="titleSize" format="reference|dimension" />
        <attr name="subTitleSize" format="reference|dimension" />
        <attr name="drawable" format="reference" />
        <attr name="arrow" format="reference" />
        <attr name="check" format="reference" />
        <attr name="image" format="reference" />
        <attr name="clickable" format="boolean" />
        <attr name="checked" format="boolean" />
        <attr name="background" format="reference" />
    </declare-styleable>
    <!-- SettingView -->

</resources>